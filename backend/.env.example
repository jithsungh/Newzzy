# Environment Variables Example
# Copy this to your .env file and replace with your actual API keys

# Primary API key
NEWS_API_KEY=your_primary_newsdata_io_api_key_here

# Backup API keys (add as many as you have)
NEWS_API_KEY2=your_second_newsdata_io_api_key_here
NEWS_API_KEY3=your_third_newsdata_io_api_key_here
NEWS_API_KEY4=your_fourth_newsdata_io_api_key_here

# MongoDB connection
MONGO_URI=mongodb://localhost:27017/news-aggregator

# Other environment variables
NODE_ENV=development
PORT=3000
